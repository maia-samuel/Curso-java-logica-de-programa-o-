import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {


    Scanner sc = new Scanner(System.in);
    
    int a, b,c,d, x1, x2;
    
    a = sc.nextInt();
    b = sc.nextInt();
    
    x1= a * b;
    
    c= sc.nextInt();
    d= sc.nextInt();
    
    x2= c * d;
    
    System.out.println("a diferen√ßa fica:" + (x1 - x2));

    sc.close();

    



}
	
	
	
